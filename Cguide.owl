        <medicationRecommendationName rdf:datatype="&xsd;string">FOLFOX</medicationRecommendationName>
        <pharmaceuticalForm rdf:datatype="&xsd;string">IV</pharmaceuticalForm>
        <medicationRecommendationDescription rdf:datatype="&xsd;string">Principles of adjuvant therapy:

